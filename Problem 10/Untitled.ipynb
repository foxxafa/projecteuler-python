   "source": [
    "def is_prime(x):\n",
    "    for i in range(2,(int(x**(1/2))+1)):\n",
    "        if (x%i==0):\n",
    "            return False\n",
    "    return True"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "75efdefc-d715-43f5-85df-85d5fb1e709f",
   "metadata": {},
   "outputs": [],
   "source": [
    "number_sum=2\n",
    "\n",
    "for i in range(3,2000001):\n",
    "    if(is_prime(i)):\n",
    "        number_sum+=i"
   ]
