   "source": [
    "def karelertoplamı(x):\n",
    "    return x*(x+1)*(2*x+1)/6\n",
    "\n",
    "def toplamınkaresi(x):\n",
    "    return (x*(x+1)/2)**2\n",
    "\n",
    "\n",
    "print(toplamınkaresi(100)-karelertoplamı(100))"
   ]
