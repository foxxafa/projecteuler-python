      "4613732\n"
     ]
    }
   ],
   "source": [
    "a=0\n",
    "b=1\n",
    "c=0\n",
    "toplam=0\n",
    "while(c<4000000):\n",
    "    c=a+b\n",
    "    a=b\n",
    "    b=c\n",
    "    if(c%2==0):\n",
    "        toplam+=c\n",
    "\n",
    "print(toplam)"
   ]
  }
